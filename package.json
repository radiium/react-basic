{
    "name": "react-basic-repo",
    "version": "0.0.0",
    "private": true,
    "license": "MIT",
    "scripts": {
        "dev": "yarn workspaces run dev",
        "build": "yarn workspaces run build",
        "lint": "yarn workspaces run lint",
        "test": "",
        "publish": "npm publish --dry-run --workspace=react-basic",
        "major": "npm version major --git-tag-version=false --allow-same-version=true --workspaces-update=false --include-workspace-root --workspace=react-basic --workspace=react-basic-doc",
        "minor": "npm version minor --git-tag-version=false --allow-same-version=true --workspaces-update=false --include-workspace-root --workspace=react-basic --workspace=react-basic-doc",
        "patch": "npm version patch --git-tag-version=false --allow-same-version=true --workspaces-update=false --include-workspace-root --workspace=react-basic --workspace=react-basic-doc"
    },
    "workspaces": [
        "packages/react-basic",
        "packages/doc"
    ]
}
