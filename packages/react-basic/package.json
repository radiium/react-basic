        "prettier": "^3.2.5",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "sass": "^1.71.1",
        "typescript": "^5.3.3",
        "vite": "^5.1.4",
        "vite-plugin-dts": "^3.7.3",
        "vite-plugin-lib-inject-css": "^2.0.0"
